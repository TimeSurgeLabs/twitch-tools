{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "fs:read-all",
      "allow": [
        {
          "path": "$TEMP/**/*"
        }
      ]
    },
    "fs:default"
  ]
}
